{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/Inc.js", "pages/Future.js", "pages/Business.js", "src/component/Footer.js",
  "src/component/FutureSlider.js", "src/component/Header.js", "src/component/HeadInfo.js", "src/component/IncSlider.js", "src/component/Layout.js", "src/component/Main.js"],
  "exclude": ["node_modules"]
}
